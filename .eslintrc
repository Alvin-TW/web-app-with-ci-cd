{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, {
        "extensions": [
          ".js",
          ".jsx"
        ]
    }]
  },
  "settings": {
    "import/resolver": {
        "webpack": {
            "config": "./config/webpack/base.config"
        }
    }
  }
}